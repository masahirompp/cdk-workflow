{
  "name": "cdk-workflow",
  "version": "1.3.1",
  "description": "cdk workflow",
  "type": "module",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "lib/src/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "xo --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test": "node -r dotenv/config --loader ts-node/esm ./test/test-cli.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masahirompp/cdk-workflow.git"
  },
  "license": "MIT",
  "author": {
    "name": "masahirompp",
    "email": "masahirompp@users.noreply.github.com",
    "url": "https://github.com/masahirompp"
  },
  "engines": {
    "node": ">=16.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/masahirompp/cdk-workflow/issues"
  },
  "homepage": "https://github.com/masahirompp/cdk-workflow#readme",
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.9",
    "aws-cdk": "^2.51.1",
    "aws-cdk-lib": "^2.51.1",
    "commitizen": "^4.2.5",
    "constructs": "^10.1.169",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "xo": "^0.53.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "xo --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "execa": "^6.1.0",
    "inquirer": "^9.1.4",
    "strip-ansi": "^7.0.1",
    "tmp-promise": "^3.0.3"
  },
  "peerDependencies": {
    "aws-cdk": "^2",
    "aws-cdk-lib": "^2",
    "constructs": "^10"
  }
}
